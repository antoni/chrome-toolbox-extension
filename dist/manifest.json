{
  "name": "Web Toolbox",
  "author": "Antoni Ro≈õciszewski",
  "description": "A set of tools for efficiently managing your web and browser experience.",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
//    "18": "img/18.png",
//    "24": "img/24.png",
//    "27": "img/27.png",
//    "36": "img/36.png",
//    "48": "img/48.png",
//    "54": "img/54.png",
//    "72": "img/72.png",
//    "96": "img/96.png",
//    "108": "img/108.png",
//    "144": "img/144.png",
//    "192": "img/192.png"
  },
  "permissions": [
    "management",
    "notifications",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
//    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
//  "options_page": "js/options.html",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "web_accessible_resources": [
    "img/18.png",
    "img/192.png",
    "img/24.png",
    "img/27.png",
    "img/36.png",
    "img/48.png",
    "img/54.png",
    "img/72.png",
    "img/96.png",
    "img/108.png",
    "img/144.png"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Z",
        "linux": "Ctrl+Shift+Z",
        "windows": "Alt+Shift+Z",
        "mac": "Command+Shift+Z"
      }
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://allegro.pl/*"
      ],
      "js": [
        "js/content-scripts/allegro.js"
      ]
    },
    {
      "matches": [
        "https://online.enel.pl/*"
      ],
      "js": [
        "js/content-scripts/enel_med.js"
      ]
    },
    {
      "matches": [
        "https://online.mbank.pl/*"
      ],
      "js": [
        "js/content-scripts/mbank.js"
      ]
    },
    {
      "matches": [
        "https://stackoverflow.com/*"
      ],
      "js": [
        "js/content-scripts/stack_overflow_sort.js"
      ]
    }
  ]
}
